{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./src/*"
      ],
      "~configs/*": [
        "./src/configs/*"
      ],
      "~middlewares/*": [
        "./src/middlewares/*"
      ],
      "~handlers/*": [
        "./src/handlers/*"
      ],
      "~lib/*": [
        "./src/lib/*"
      ],
      "~modules/*": [
        "./src/modules/*"
      ],
      "~resources/*": [
        "./src/resources/*"
      ],
      "~utils/*": [
        "./src/utils/*"
      ],
      "~routes/*": [
        "./src/routes/*"
      ],
      "~services/*": [
        "./src/services/*"
      ]
    }
  }
}