{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./src/*"
      ],
      "~configs/*": [
        "./src/configs/*"
      ],
      "~middlewares/*": [
        "./src/middlewares/*"
      ],
      "~controllers/*": [
        "./src/controllers/*"
      ],
      "~lib/*": [
        "./src/lib/*"
      ],
      "~resources/*": [
        "./src/resources/*"
      ],
      "~utils/*": [
        "./src/utils/*"
      ],
      "~routes/*": [
        "./src/routes/*"
      ],
      "~services/*": [
        "./src/services/*"
      ],
      "~repositories/*": [
        "./src/repositories/*"
      ],
      "~models/*": [
        "./src/models/*"
      ]
    }
  }
}